{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/free-nitro-real-100-working-2023-no-survey",
        "destination": "https://youtu.be/dQw4w9WgXcQ",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self'; connect-src 'self' randomfox.ca; img-src 'self' randomfox.ca"
          }
        ]
      }
    ]
  }
}
